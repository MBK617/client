{
  "templates": {
    "applicationName": "MBK617-mobile",
    "linenums": true
  },
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "tutorials": "help",
    "encoding": "utf8",
    "recurse": true,
    "private": false,
    "lenient": true,
    "destination": "docs",
    "template": "node_modules/@pixi/jsdoc-template"
  }
}